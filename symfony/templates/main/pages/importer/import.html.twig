<form id="form-import-filter">

    <div class="form-group">
        <label for="select-sport">Select sport</label>
        <select name="sport-ids" class="form-control" sport-select id="select-sport"></select>
    </div>

    <div class="form-group">
        <label for="after-sport">After (timestamp UTC auto generated)</label>
        <input name="after" type='text' class="form-control" id='after-sport'/>
    </div>

    <button type="submit" class="btn btn-primary">Search</button>
</form>
<div style="text-align: right"><button id="start-import" class="btn btn-primary">Start Import !</button></div>
<div class="row" id="display-events-import"></div>